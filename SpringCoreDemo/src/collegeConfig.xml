<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="stdnt" class="com.arun.spring.college.Student">
		<property name="id" value="1001"></property>
		<property name="name" value="arun"></property>
		<property name="age" value="26"></property>
		<property name="course" ref="comp"></property>
	</bean>


	<bean id="bio" class="com.arun.spring.college.BioScienceCourse"></bean>
	<bean id="comp" class="com.arun.spring.college.CompScienceCourse"></bean>
	<bean id="comm" class="com.arun.spring.college.CommerceCourse"></bean>
</beans>